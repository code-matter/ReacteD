// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Color {
  id String @id @default(cuid())
  color String
  reactionTime Int
  time String
}

model ColorDev {
  id           String @id @default(cuid())
  color        String
  reactionTime Int
  time         String
}